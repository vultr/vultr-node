{
  "name": "@vultr/vultr-node",
  "private": false,
  "version": "2.0.1",
  "description": "Node module to communicate with the Vultr API",
  "main": "src/index.js",
  "scripts": {
    "build": "babel src -d dist",
    "test": "jest",
    "lint": "eslint src",
    "prettier": "prettier --write src",
    "docs": "jsdoc --readme ./README.md ./src/api -r -d docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vultr/vultr-node.git"
  },
  "keywords": [
    "vultr",
    "node",
    "api"
  ],
  "author": "Vultr",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vultr/vultr-node/issues"
  },
  "homepage": "https://github.com/vultr/vultr-node#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run prettier && npm test",
      "pre-push": "npm run lint && npm test"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "eslint": "^7.3.1",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^4.2.3",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.3",
    "prettier": "^2.0.5"
  },
  "dependencies": {
    "node-fetch": "^2.6.0"
  },
  "directories": {
    "test": "test"
  },
  "publishConfig": {
    "access": "public"
  }
}
